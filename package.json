{
  "name": "metadata-frontend",
  "version": "1.0.0",
  "description": "Group Robot",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged && npm run test:metadata",
    "start": "cross-env APP_TYPE=site PORT=9488 umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "build": "cross-env UMI_ENV=prod umi build",
    "build-meta-lib": "npm run test:metadata && webpack --config src/meta-component/webpack.config.js",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src && npm run lint:style",
    "lint:fix-meta": "eslint --fix --ext .js src/meta-component",
    "lint:fix-meta-engine": "eslint --fix --ext .js src/meta-component/src/metadataComponentEngine",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js src/meta-component/**/*.js",
    "prettier": "prettier --write ./src/**/**/**/*",
    "prettier:meta": "prettier --write ./src/meta-component/**/*.js",
    "test": "umi test",
    "test:metadata": "umi test ./src/meta-component",
    "debug": "node --inspect-brk ./node_modules/.bin/umi test"
  },
  "dependencies": {
    "@babel/runtime": "7.3.4",
    "add": "^2.0.6",
    "antd": "3.13.5",
    "axios": "0.18.0",
    "classnames": "2.2.6",
    "dva": "2.4.1",
    "enquire-js": "0.2.1",
    "global": "^4.3.2",
    "hash.js": "1.1.7",
    "history": "4.7.2",
    "invariant": "latest",
    "isomorphic-fetch": "2.2.1",
    "locks": "0.2.2",
    "lodash": "4.17.11",
    "lodash-decorators": "6.0.1",
    "memoize-one": "5.0.0",
    "moment": "2.24.0",
    "npm": "6.13.4",
    "numeral": "2.0.6",
    "nzh": "1.0.4",
    "omit.js": "1.0.0",
    "path-to-regexp": "3.0.0",
    "prop-types": "15.7.2",
    "qs": "6.6.0",
    "rc-animate": "2.6.0",
    "react": "16.8.4",
    "react-container-query": "0.11.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-document-title": "2.0.3",
    "react-dom": "16.8.4",
    "react-fittext": "1.0.0",
    "react-loadable": "5.5.0",
    "react-redux": "6.0.1",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-saga": "1.0.2",
    "umi": "2.5.5",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "antd-pro-merge-less": "1.0.0",
    "antd-pro-theme-webpack-plugin": "1.1.8",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "7.1.5",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-es2015-arrow-functions": "6.22.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "6.22.0",
    "babel-plugin-transform-es2015-block-scoping": "6.26.0",
    "babel-plugin-transform-es2015-classes": "6.24.1",
    "babel-plugin-transform-es2015-computed-properties": "6.24.1",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-es2015-for-of": "6.23.0",
    "babel-plugin-transform-es2015-function-name": "6.24.1",
    "babel-plugin-transform-es2015-literals": "6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-es2015-object-super": "6.24.1",
    "babel-plugin-transform-es2015-parameters": "6.24.1",
    "babel-plugin-transform-es2015-shorthand-properties": "6.24.1",
    "babel-plugin-transform-es2015-spread": "6.22.0",
    "babel-plugin-transform-es2015-sticky-regex": "6.24.1",
    "babel-plugin-transform-es2015-template-literals": "6.22.0",
    "babel-plugin-transform-es2015-unicode-regex": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-display-name": "6.25.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "5.2.0",
    "cross-port-killer": "1.0.1",
    "css-loader": "2.1.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "5.15.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-compat": "2.7.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-markdown": "1.0.0",
    "eslint-plugin-react": "7.12.4",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.1.0",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "lint-staged": "8.1.5",
    "merge-umi-mock-data": "1.0.4",
    "mini-css-extract-plugin": "0.5.0",
    "mockjs": "1.0.1-beta3",
    "postcss-cssnext": "3.1.0",
    "postcss-nested": "4.1.2",
    "postcss-simple-vars": "5.0.2",
    "prettier": "1.16.4",
    "pro-download": "1.0.1",
    "react-scripts": "2.1.5",
    "stompjs": "2.3.3",
    "style-loader": "0.23.1",
    "stylelint": "9.10.1",
    "stylelint-config-prettier": "5.0.0",
    "stylelint-config-standard": "18.2.0",
    "umi-plugin-ga": "1.1.3",
    "umi-plugin-react": "1.5.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-node-externals": "1.7.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/src/meta-component/src/tests/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
